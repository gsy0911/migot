{
 "name": "migot",
 "version": "0.1.0",
 "description": "productivity",
 "main": "dist/main.js",
 "scripts": {
  "dev": "ts-node src/index.ts",
  "dev:watch": "ts-node-dev --respawn src/index.ts",
  "clean": "rimraf dist/*",
  "tsc": "tsc",
  "webpack": "webpack",
  "build": "npm-run-all clean lint:fix tsc webpack",
  "start": "electron ./dist/main.js --debug",
  "check-types": "tsc --noEmit",
  "eslint": "eslint src/**/*.ts",
  "eslint:fix": "eslint src/**/*.ts --fix",
  "format": "prettier --write 'src/**/*.{js,ts,json}'",
  "lint": "npm-run-all eslint check-types",
  "lint:fix": "npm-run-all eslint:fix check-types format",
  "as:mac": "electron-builder --mac --x64",
  "package:mac": "npm-run-all build as:mac"
 },
 "repository": {
  "type": "git",
  "url": "git+https://github.com/gsy0911/migot.git"
 },
 "keywords": [],
 "author": "",
 "license": "MIT",
 "bugs": {
  "url": "https://github.com/gsy0911/migot/issues"
 },
 "homepage": "https://github.com/gsy0911/migot#readme",
 "devDependencies": {
  "@commitlint/cli": "^11.0.0",
  "@commitlint/config-conventional": "^11.0.0",
  "@types/node": "^14.14.21",
  "@types/react": "^17.0.0",
  "@types/react-dom": "^17.0.0",
  "@types/react-redux": "^7.1.15",
  "@types/styled-components": "^5.1.7",
  "@typescript-eslint/eslint-plugin": "^4.14.0",
  "@typescript-eslint/parser": "^4.14.0",
  "copy-webpack-plugin": "^7.0.0",
  "css-loader": "^5.0.1",
  "electron": "^11.2.0",
  "electron-builder": "^22.9.1",
  "eslint": "^7.18.0",
  "eslint-config-prettier": "^7.2.0",
  "html-webpack-plugin": "^4.5.1",
  "husky": "^4.3.8",
  "lint-staged": "^10.5.3",
  "npm-run-all": "^4.1.5",
  "prettier": "^2.2.1",
  "rimraf": "^2.7.1",
  "style-loader": "^2.0.0",
  "ts-loader": "^8.0.14",
  "ts-node": "^9.1.1",
  "ts-node-dev": "^1.1.1",
  "typescript": "^4.1.3",
  "webpack": "^5.16.0",
  "webpack-cli": "^4.4.0"
 },
 "husky": {
  "hooks": {}
 },
 "lint-staged": {
  "*.{js,ts}": "eslint --cache --fix",
  "*.ts": "tsc --noEmit",
  "*.{js,ts,json}": "prettier --write"
 },
 "build": {
  "extends": null,
  "productName": "migot",
  "appId": "migot.mdrskn.com",
  "directories": {
   "output": "./products"
  },
  "files": [
   "dist/**/*"
  ],
  "dmg": {
   "contents": [
    {
     "x": 410,
     "y": 150,
     "type": "link",
     "path": "/Applications"
    },
    {
     "x": 130,
     "y": 150,
     "type": "file"
    }
   ]
  },
  "mac": {
   "icon": "./src/icon/mac_icon.icns",
   "target": [
    "dmg"
   ]
  }
 },
 "dependencies": {
  "@date-io/date-fns": "^1.3.13",
  "@material-ui/core": "^4.11.3",
  "@material-ui/icons": "^4.11.2",
  "@material-ui/pickers": "^3.2.10",
  "@types/react-router-dom": "^5.1.7",
  "date-fns": "^2.16.1",
  "fs-extra": "^9.1.0",
  "nanoid": "^3.1.20",
  "react": "^17.0.1",
  "react-dom": "^17.0.1",
  "react-redux": "^7.2.2",
  "react-router": "^5.2.0",
  "react-router-dom": "^5.2.0",
  "redux": "^4.0.5",
  "reset-css": "^5.0.1",
  "styled-components": "^5.2.1",
  "typescript-fsa": "^3.0.0",
  "typescript-fsa-reducers": "^1.2.2"
 }
}
